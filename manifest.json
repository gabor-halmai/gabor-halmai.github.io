{
	"_version": "1.21.0",
	"sap.app": {
		"id": "com.otisoft.favs.Favorites",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				"",
				"en"
			]
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"favorites_ds": {
				"uri": "model/favorites.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": true,
		"rootView": {
			"viewName": "com.otisoft.favs.Favorites.view.MainView",
			"type": "XML",
			"async": true,
			"id": "idAppControl"
		},
		"dependencies": {
			"minUI5Version": "1.143.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {
					"lazy": false
				},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.otisoft.favs.Favorites.i18n.i18n",
					"supportedLocales": [
						"",
						"en"
					]
				}
			},
			"favorites": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "favorites_ds",
				"preload": true,
				"settings": {
					"defaultBindingMode": "OneTime"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.otisoft.favs.Favorites.view",
				"controlId": "idAppControl",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"name": "RouteMainView",
					"pattern": "RouteMainView",
					"target": [
						"TargetMainView"
					]
				}
			],
			"targets": {
				"TargetMainView": {
					"viewType": "XML",
					"viewLevel": 1,
					"viewId": "idAppControl",
					"viewName": "MainView"
				}
			}
		},
		"flexBundle": false
	}
}